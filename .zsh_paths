# -------------------------------
# Matt's Paths
# -------------------------------

# Flutter
[ -d "$HOME/FlutterDev/flutter/bin" ] && export PATH="$PATH:$HOME/FlutterDev/flutter/bin"

# Node.js (NVM)
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"

# Android SDK
export ANDROID_HOME="$HOME/Library/Android/sdk"
for d in emulator tools tools/bin platform-tools; do
  [ -d "$ANDROID_HOME/$d" ] && export PATH="$PATH:$ANDROID_HOME/$d"
done

# Conda (auto-generated by conda init)
__conda_setup="$('/Users/matt/Downloads/ENTER/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/matt/Downloads/ENTER/etc/profile.d/conda.sh" ]; then
        . "/Users/matt/Downloads/ENTER/etc/profile.d/conda.sh"
    else
        export PATH="/Users/matt/Downloads/ENTER/bin:$PATH"
    fi
fi
unset __conda_setup

# RVM
[ -d "$HOME/.rvm/bin" ] && export PATH="$PATH:$HOME/.rvm/bin"

# Dart pub cache
[ -d "$HOME/.pub-cache/bin" ] && export PATH="$PATH:$HOME/.pub-cache/bin"

# Windsurf
[ -d "$HOME/.codeium/windsurf/bin" ] && export PATH="$HOME/.codeium/windsurf/bin:$PATH"

# Docker completions
fpath=(/Users/matt/.docker/completions $fpath)

